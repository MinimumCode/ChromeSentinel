{
  "manifest_version": 2,

  "name": "Chrome Sentinel",
  "description": "Extension for retrieving web browser usage",
  "version": "1.0",
  "background": {
    "scripts":["sentinel.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions":[
    "tabs"
  ]
}